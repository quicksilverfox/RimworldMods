					<min>3</min>
					<max>3</max>
				</thingDefCountRange>
				<countRange>
					<min>1</min>
					<max>1</max>
				</countRange>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>Artifact</tradeTag>
				<thingDefCountRange>
					<min>2</min>
					<max>2</max>
				</thingDefCountRange>
				<countRange>
					<min>1</min>
					<max>1</max>
				</countRange>
			</li>
			<li Class="StockGenerator_Art">
				<countRange>
					<min>2</min>
					<max>2</max>
				</countRange>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>Television</tradeTag>
				<thingDefCountRange>
					<min>1</min>
					<max>2</max>
				</thingDefCountRange>
				<countRange>
					<min>1</min>
					<max>1</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Telescope</thingDef>
				<countRange>
					<min>0</min>
					<max>1</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>LeeaniBookshelf</thingDef>
				<countRange>
					<min>-3</min>
					<max>1</max>
				</countRange>
			</li>
			<li Class="StockGenerator_Animals">
				<checkTemperature>true</checkTemperature>
				<tradeTagsSell>
					<li>AnimalUncommon</li>
					<li>AnimalFighter</li>
				</tradeTagsSell>
				<tradeTagsBuy>
					<li>AnimalExotic</li>
				</tradeTagsBuy>
				<kindCountRange>
					<min>3</min>
					<max>3</max>
				</kindCountRange>
				<countRange>
					<min>1</min>
					<max>3</max>
				</countRange>
			</li>

			<!-- Buying -->
			<li Class="StockGenerator_BuyExpensiveSimple" />
			<li Class="StockGenerator_BuyTradeTag">
				<tag>Art</tag>
			</li>
			<li Class="StockGenerator_BuyTradeTag">
				<tag>HiTechArmor</tag>
			</li>
			<li Class="StockGenerator_BuyTradeTag">
				<tag>ImplantEmpireCommon</tag>
			</li>
			<li Class="StockGenerator_BuyTradeTag">
				<tag>ImplantEmpireRoyal</tag>
			</li>
			<li Class="StockGenerator_BuyTradeTag">
				<tag>ExoticBuilding</tag>
			</li>
		</stockGenerators>
	</TraderKindDef>

</Defs>
