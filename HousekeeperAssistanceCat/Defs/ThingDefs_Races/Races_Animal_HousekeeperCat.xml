			<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
			<ageGenerationCurve>
				<points>
					<li>(1,0)</li>
					<li>(7,20)</li>
					<li>(15,60)</li>
					<li>(20,20)</li>
					<li>(25,0)</li>
				</points>
			</ageGenerationCurve>
		</race>
		<tradeTags>
			<li>AnimalUncommon</li>
		</tradeTags>
		<recipes>
			<li>ExciseCarcinoma</li>
			<li>AdministerMechSerumHealer</li>
