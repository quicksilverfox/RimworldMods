<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<TraderKindDef>
		<defName>Caravan_Leeani_BulkGoods</defName>
		<label>bulk goods trader</label>
		<commonality>2</commonality>
		<stockGenerators>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Silver</thingDef>
				<countRange>
					<min>1000</min>
					<max>1800</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>ComponentIndustrial</thingDef>
				<countRange>
					<min>15</min>
					<max>20</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>ComponentSpacer</thingDef>
				<countRange>
					<min>-10</min>
					<max>1</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Steel</thingDef>
				<countRange>
					<min>300</min>
					<max>500</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Cloth</thingDef>
				<countRange>
					<min>300</min>
					<max>500</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Chemfuel</thingDef>
				<countRange>
					<min>250</min>
					<max>400</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Neutroamine</thingDef>
				<countRange>
					<min>20</min>
					<max>50</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Beer</thingDef>
				<totalPriceRange>
					<min>-100</min>
					<max>100</max>
				</totalPriceRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>LeeaniCider</thingDef>
				<countRange>
					<min>50</min>
					<max>200</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>LeeaniWine</thingDef>
				<countRange>
					<min>50</min>
					<max>200</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Chocolate</thingDef>
				<totalPriceRange>
					<min>20</min>
					<max>100</max>
				</totalPriceRange>
			</li>
			<li Class="StockGenerator_Category">
				<categoryDef>FoodRaw</categoryDef>
				<excludedCategories>
					<li>EggsFertilized</li>
				</excludedCategories>
				<thingDefCountRange>
					<min>3</min>
					<max>3</max>
				</thingDefCountRange>
				<totalPriceRange>
					<min>200</min>
					<max>800</max>
				</totalPriceRange>
			</li>
			<li Class="StockGenerator_Category">
				<categoryDef>BuildingsFurniture</categoryDef>
				<thingDefCountRange>
					<min>2</min>
					<max>2</max>
				</thingDefCountRange>
				<countRange>
					<min>1</min>
					<max>1</max>
				</countRange>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>LeeaniFurniture</tradeTag>
				<thingDefCountRange>
					<min>1</min>
					<max>2</max>
				</thingDefCountRange>
				<countRange>
					<min>1</min>
					<max>1</max>
				</countRange>
			</li>
			<li Class="StockGenerator_Animals">
				<maxWildness>0.70</maxWildness>
				<tradeTagsSell>
					<li>AnimalFarm</li>
					<li>AnimalPet</li>
				</tradeTagsSell>
				<tradeTagsBuy>
					<li>AnimalExotic</li>
				</tradeTagsBuy>
				<kindCountRange>
					<min>1</min>
					<max>1</max>
				</kindCountRange>
				<countRange>
					<min>4</min>
					<max>5</max>
				</countRange>
			</li>
			<li Class="StockGenerator_Clothes">
				<countRange>
					<min>10</min>
					<max>10</max>
				</countRange>
			</li>
			<li Class="StockGenerator_Category">
				<categoryDef>Drugs</categoryDef>
			</li>
			<li Class="StockGenerator_Category">
				<categoryDef>Textiles</categoryDef>
			</li>
			<li Class="StockGenerator_MultiDef">
				<thingDefs>
					<li>Chocolate</li>
					<li>Kibble</li>
					<li>Pemmican</li>
				</thingDefs>
			</li>

			<!-- Buying -->
			<li Class="StockGenerator_BuyExpensiveSimple" />
			<li Class="StockGenerator_BuyTradeTag">
				<tag>Art</tag>
			</li>
			<li Class="StockGenerator_BuyTradeTag">
				<tag>HiTechArmor</tag>
			</li>
			<li Class="StockGenerator_BuyTradeTag">
				<tag>ImplantEmpireCommon</tag>
			</li>
			<li Class="StockGenerator_BuyTradeTag">
				<tag>ImplantEmpireRoyal</tag>
			</li>
			<li Class="StockGenerator_BuyTradeTag">
				<tag>ExoticBuilding</tag>
			</li>
		</stockGenerators>
	</TraderKindDef>

	<TraderKindDef>
		<defName>Caravan_Leeani_CombatSupplier</defName>
		<label>combat supplier</label>
		<commonality>1</commonality>
		<stockGenerators>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Silver</thingDef>
				<countRange>
					<min>750</min>
					<max>1200</max>
				</countRange>
			</li>
			<li Class="StockGenerator_WeaponsRanged">
				<countRange>
					<min>5</min>
					<max>5</max>
				</countRange>
			</li>
			<li Class="StockGenerator_Category">
				<categoryDef>WeaponsMelee</categoryDef>
